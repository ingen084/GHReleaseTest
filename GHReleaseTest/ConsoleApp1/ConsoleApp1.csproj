<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
	<AssemblyVersion>$(APP_VERSION).$(GITHUB_RUN_NUMBER)</AssemblyVersion>
	<CFBundleShortVersionString>$(APP_VERSION).$(GITHUB_RUN_NUMBER)</CFBundleShortVersionString>
